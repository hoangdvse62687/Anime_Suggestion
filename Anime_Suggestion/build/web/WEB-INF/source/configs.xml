        <topWeekClassName>Title</topWeekClassName>
        <topWeekNameElement>div</topWeekNameElement>
        <topWeekCommentClass>div</topWeekCommentClass>
        <topWeekCommentElement>div</topWeekCommentElement>
    </config>
    
    <config>
        <uri>https://anime47.com</uri>
         Format category 
        <categoryContext>thể loại</categoryContext>
