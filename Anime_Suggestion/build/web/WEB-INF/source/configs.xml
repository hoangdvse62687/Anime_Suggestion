         Format Data 
        <parentElementMovie>li</parentElementMovie>
        <parentClassMovie>TPostMv</parentClassMovie>
        <imgElementMovie>img</imgElementMovie>
        <imgClassMovie>attachment-thumbnail size-thumbnail wp-post-image</imgClassMovie>
        <imgTargetMovie>src</imgTargetMovie>
        <nameClassMovie>Title</nameClassMovie>
        <nameTargetMovie>h2</nameTargetMovie>
        <commentClassMovie></commentClassMovie>
        <commentTargetMovie></commentTargetMovie>
        <viewedClassMovie>Year</viewedClassMovie>
        <viewedTargetMovie>span</viewedTargetMovie>
        <shortenMovieDisplayOrder>href</shortenMovieDisplayOrder>
        <shortenMovieDisplayOrder>img</shortenMovieDisplayOrder>
        <shortenMovieDisplayOrder>name</shortenMovieDisplayOrder>
        <shortenMovieDisplayOrder>comment</shortenMovieDisplayOrder>
        <shortenMovieDisplayOrder>viewed</shortenMovieDisplayOrder>
        Get Data Format
        <episodeClassMovieDetail>Time AAIco-access_time</episodeClassMovieDetail>
        <episodeTargetMovieDetail>span</episodeTargetMovieDetail>
        <episodeContextMovideDetail></episodeContextMovideDetail>
        <typeClassMovieDetail>AAIco-adjust</typeClassMovieDetail>
        <typeTargetMovieDetail>li</typeTargetMovieDetail>
        <yearClassMovieDetail>Date AAIco-date_range</yearClassMovieDetail>
        <yearTargetMovieDetail>span</yearTargetMovieDetail>
        <yearContextMovieDetail></yearContextMovieDetail>
        <descriptionClassMovieDetail>Description</descriptionClassMovieDetail>
        <descriptionTargetMovieDetail>div</descriptionTargetMovieDetail>
        <descriptionContextMovieDetail></descriptionContextMovieDetail>
        
        <dumplicateClassMovieDetail>AAIco-adjust</dumplicateClassMovieDetail>
        <dumplicateTagetMovideDetail>li</dumplicateTagetMovideDetail>
        
        <detailMovieDisplayOrder>description</detailMovieDisplayOrder>
        <detailMovieDisplayOrder>episode</detailMovieDisplayOrder>
        <detailMovieDisplayOrder>year</detailMovieDisplayOrder>
        <detailMovieDisplayOrder>dump</detailMovieDisplayOrder>
        <detailMovieDisplayOrder>type</detailMovieDisplayOrder>
        Trend in week format
        <topWeekNameCLassParent>id</topWeekNameCLassParent>
        <topWeekValueClassParent>showTopPhim</topWeekValueClassParent>
        <topWeekElementName>section</topWeekElementName>
        <topWeekClassUri>MovieList</topWeekClassUri>
        <topWeekTargetUri>ul</topWeekTargetUri>
        
        <maximumPageGetInCategory>4</maximumPageGetInCategory>
