        <parentElementMovie>div</parentElementMovie>
        <parentClassMovie>movie-item m-block</parentClassMovie>
        <imgElementMovie>div</imgElementMovie>
        <imgClassMovie>public-film-item-thumb ratio-content</imgClassMovie>
        <imgTargetMovie>style</imgTargetMovie>
        <nameClassMovie>movie-title-1</nameClassMovie>
        <nameTargetMovie>div</nameTargetMovie>
        <commentClassMovie>fbcom-left</commentClassMovie>
        <commentTargetMovie>span</commentTargetMovie>
        <viewedClassMovie>viewed-right</viewedClassMovie>
        <viewedTargetMovie>span</viewedTargetMovie>
        
        <shortenMovieDisplayOrder>href</shortenMovieDisplayOrder>
        <shortenMovieDisplayOrder>img</shortenMovieDisplayOrder>
        <shortenMovieDisplayOrder>name</shortenMovieDisplayOrder>
        <shortenMovieDisplayOrder>comment</shortenMovieDisplayOrder>
        <shortenMovieDisplayOrder>viewed</shortenMovieDisplayOrder>
        Format Get Detail Data
        <episodeClassMovieDetail>movie-dd imdb</episodeClassMovieDetail>
        <episodeTargetMovieDetail>dd</episodeTargetMovieDetail>
        <episodeContextMovideDetail></episodeContextMovideDetail>
        <typeClassMovieDetail>movie-dd dd-cat</typeClassMovieDetail>
        <typeTargetMovieDetail>dd</typeTargetMovieDetail>
        <yearClassMovieDetail>movie-dd</yearClassMovieDetail>
        <yearTargetMovieDetail>dd</yearTargetMovieDetail>
        <yearContextMovieDetail></yearContextMovieDetail>
        <descriptionClassMovieDetail>news-article</descriptionClassMovieDetail>
        <descriptionTargetMovieDetail>div</descriptionTargetMovieDetail>
        <descriptionContextMovieDetail></descriptionContextMovieDetail>
        
        <dumplicateClassMovieDetail>movie-dd</dumplicateClassMovieDetail>
        <dumplicateTagetMovideDetail>dd</dumplicateTagetMovideDetail>
        
        <detailMovieDisplayOrder>episode</detailMovieDisplayOrder>
        <detailMovieDisplayOrder>type</detailMovieDisplayOrder>
        <detailMovieDisplayOrder>dump</detailMovieDisplayOrder>
        <detailMovieDisplayOrder>dump</detailMovieDisplayOrder>
        <detailMovieDisplayOrder>year</detailMovieDisplayOrder>
        <detailMovieDisplayOrder>description</detailMovieDisplayOrder>
        Trend in week format
        <topWeekNameCLassParent>data-name</topWeekNameCLassParent>
        <topWeekValueClassParent>month</topWeekValueClassParent>
        <topWeekElementName>div</topWeekElementName>
        <topWeekClassUri>list-top-movie</topWeekClassUri>
        <topWeekTargetUri>ul</topWeekTargetUri>
        
        <maximumPageGetInCategory>3</maximumPageGetInCategory>
         Forbidden link categories 
        <forbiddenLinkCategories>/tui-deo-balo-vi-pc158230.html</forbiddenLinkCategories>
        
        <topWeekImgClass>list-top-movie-item-thumb</topWeekImgClass>
        <topWeekImgClassTarget>style</topWeekImgClassTarget>
        <topWeekClassName>list-top-movie-item-vn</topWeekClassName>
        <topWeekNameElement>span</topWeekNameElement>
        <topWeekCommentClass>list-top-movie-item-view</topWeekCommentClass>
        <topWeekCommentElement>span</topWeekCommentElement>
    </config>
